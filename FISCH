                        vim:SendKeyEvent(false, Enum.KeyCode.E, false, game)
                        FreezeSuspend()
                        task.wait(0.1)
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1311, -802, -83)
                        task.wait(0.1)
                        FreezeResume()
