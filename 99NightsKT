-- Initialize crafting tracker
DataCraft = require(game:GetService("ReplicatedStorage").Databases.CraftingDatabase)

function getwoodprice(name)
for ii,vv in pairs(DataCraft.PossibleBlueprints) do
	for i,v in pairs(vv) do
		if v.Name == name then
			return v.WoodPrice or 0 , v.ScrapPrice or 0
		end
	end
end
return nil
end

