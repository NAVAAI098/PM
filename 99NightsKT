_G.Craft = _G.Craft or {}
spawn(function()
    while task.wait(.4) do
        

        -- List of items to craft
        local itemsToCraft = {
            "Crafting Bench 2",
            "Old Bed",
            "Regular Bed",
            "Crafting Bench 3",
            "Good Bed",
            "Crafting Bench 4",
            "Giant Bed",
            "Farm Plot"
        }

        -- Get CraftItem remote
        local CraftItemRemote = game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("CraftItem")

        -- Loop through items and craft each
        for _, itemName in ipairs(itemsToCraft) do
            if not _G.Craft[itemName] or itemName == "Farm Plot" then
               --
               if getwoodprice(itemName) then
                    a,b = getwoodprice(itemName)
                    if workspace.Map.Campground:GetAttribute("TotalWood") >= a + 10 and workspace.Map.Campground:GetAttribute("TotalScrap") >= b + 10 then
                        cc = CraftItemRemote:InvokeServer(itemName)
                        print("Crafting ",itemName , cc)
                        if cc == true then
                            _G.Craft[itemName] = true
                            sessionStats.itemsCrafted = sessionStats.itemsCrafted + 1
                            print("Crafted " , itemName)

                            --
                            task.wait(.2)
                            for i,v in pairs(game.Players.LocalPlayer.Inventory:GetChildren()) do
                                --
                                local args = {
                                    v,
                                    Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X + math.random(0,10) , workspace.Map.Ground.Grass.CFrame.Y , game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y + math.random(0,10)),
                                    CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X + math.random(0,10) , workspace.Map.Ground.Grass.CFrame.Y , game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y + math.random(0,10)),
                                    true
                                }
                                a = game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestPlaceStructure"):InvokeServer(unpack(args))
                                
                                if typeof(a) == "table" then
                                    table.foreach(a , print)
                                end
                            end
                        end
                    end
               else
               end
            end
        end 
    end
end)


spawn(function()
    while task.wait(.2) do
        if _G.AutoEat then
            Zero = 0
            for i,v in pairs(game:GetService("Players").LocalPlayer.Inventory:GetChildren()) do
                if v:GetAttribute("RestoreHealth") and game.Players.LocalPlayer.Character.Humanoid.Health < 100 then
                    local args = {
                        v
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestConsumeItem"):InvokeServer(unpack(args))                    
                end
            end
            for i,v in pairs(workspace.Items:GetChildren()) do
                if v:GetAttribute("Cookable") then
                    local args = {
                        workspace:WaitForChild("Map"):WaitForChild("Campground"):WaitForChild("MainFire"),
                        v
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestCookItem"):FireServer(unpack(args))
                end

                task.spawn(function()
                    if v:GetAttribute("RestoreHealth") and game.Players.LocalPlayer.Character.Humanoid.Health < 100 then
                        local args = {
                            v
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestHotbarItem"):InvokeServer(unpack(args))                    
                    end
                end)
            end
            local closestItem = nil
            local shortestDistance = math.huge

            for _, item in pairs(workspace.Items:GetChildren()) do
                if item:GetAttribute("RestoreHunger") and not item:GetAttribute("Cookable") and (item.Name == "Cooked Morsel" or item.Name == "Cooked Steak") then
                    if item:IsA("Model") and item.PrimaryPart then
                        local character = game:GetService("Players").LocalPlayer.Character or game:GetService("Players").LocalPlayer.CharacterAdded:Wait()
                        local hrp = character:WaitForChild("HumanoidRootPart")
                        local distance = (item:GetPivot().Position - hrp.Position).Magnitude
                        if distance < shortestDistance then
                            shortestDistance = distance
                            closestItem = item
                        end
                    end
                end
            end

            if closestItem and game:GetService("Players").LocalPlayer:GetAttribute("Hunger") < 150 then
                local character = game:GetService("Players").LocalPlayer.Character or game:GetService("Players").LocalPlayer.CharacterAdded:Wait()
                local hrp = character:WaitForChild("HumanoidRootPart")

                local dragArgs = {
                    closestItem
                }
                game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestStartDraggingItem"):FireServer(unpack(dragArgs))

                closestItem:PivotTo(hrp.CFrame * CFrame.new(0, 2, 0))

                wait(0.3)

                game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("StopDraggingItem"):FireServer(unpack(dragArgs))

                local consumeArgs = {
                    closestItem
                }
                game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestConsumeItem"):InvokeServer(unpack(consumeArgs))

                if sessionStats and typeof(sessionStats.foodEaten) == "number" then
                    sessionStats.foodEaten = sessionStats.foodEaten + 1
                end
            end
        end
    end
end)

