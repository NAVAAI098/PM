local TweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local camera = workspace.CurrentCamera

local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local FogFolder = workspace.Map.Boundaries.Fog
local SPEED = 1000 -- studs/‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤ Part ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function getAllParts(parent)
    local parts = {}
    for _, obj in ipairs(parent:GetDescendants()) do
        if obj:IsA("BasePart") then
            table.insert(parts, obj)
        end
    end
    return parts
end

local allParts = getAllParts(FogFolder)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Tween ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÑ‡∏õ‡∏´‡∏≤ Part ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
local function tweenToPart(targetPart, callback)
    if not _G.Tween then return end
    _G.NoClip = true

    local startPos = humanoidRootPart.Position
    local targetPos = targetPart.Position + Vector3.new(0, 3, 0)

    local distance = (targetPos - startPos).Magnitude
    local duration = distance / SPEED

    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
    local tween = TweenService:Create(humanoidRootPart, tweenInfo, { CFrame = CFrame.new(targetPos) })

    tween.Completed:Connect(function()
        if _G.Tween and callback then
            callback()
        end
    end)

    tween:Play()
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏ô‡πÑ‡∏õ‡∏ó‡∏∏‡∏Å Part ‡∏ó‡∏µ‡∏•‡∏∞‡∏ï‡∏±‡∏ß
local currentIndex = 1
local function moveToNextPart()
    if not _G.Tween then return end

    if currentIndex > #allParts then
        currentIndex = 1
    end

    local targetPart = allParts[currentIndex]
    currentIndex = currentIndex + 1

    tweenToPart(targetPart, moveToNextPart)
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô Tween
local function startTween()
    if not _G.Tween then
        return
    end
    if #allParts > 0 then
        moveToNextPart()
    end
end


local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
