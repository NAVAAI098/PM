local TargetNames = { "Bunny","Bear","Alpha Wolf", "Wolf","Crossbow Cultist", "Cultist", "Deer" }


task.spawn(function()
	while task.wait(.2) do
		pcall(function()
			if _G.KillAura then
				if not game:GetService("Players").LocalPlayer.Character or not game:GetService("Players").LocalPlayer:FindFirstChild("Inventory") then return end

				for _, tool in pairs(game:GetService("Players").LocalPlayer:FindFirstChild("Inventory"):GetChildren()) do
					if tool:GetAttribute("Owner") then
						for _, model in pairs(workspace:WaitForChild("Characters"):GetChildren()) do
							if model ~= game:GetService("Players").LocalPlayer.Character 
								and model:FindFirstChildOfClass("Humanoid") 
								and model:FindFirstChildOfClass("Humanoid").Health > 0 
								and model:FindFirstChild("HumanoidRootPart") 
								and game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") 
								and not model:GetAttribute("NotAttackable") then
								
								if (game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position - model:FindFirstChild("HumanoidRootPart").Position).Magnitude <= 300 then
									task.spawn(function()
										game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ToolDamageObject"):InvokeServer(
											model,
											tool,
											"1_" .. tostring(tool:GetAttribute("Owner")),
											model:FindFirstChild("HumanoidRootPart").CFrame
										)
                                        task.wait(.1)
									end)
                                    
								end
							end
						end
					end
				end
			end
		end)
	end
end)



startTween()


