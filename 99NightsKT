-- Animation functions
local function createPulseAnimation(object)
    local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local tween = TweenService:Create(object, tweenInfo, {
        TextStrokeTransparency = 0.5
    })
    tween:Play()
end

local function createFloatAnimation(object)
    local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true)
    local originalPosition = object.Position
    local tween = TweenService:Create(object, tweenInfo, {
        Position = originalPosition + UDim2.new(0, 0, 0, -10)
    })
    tween:Play()
end

-- Apply animations
createPulseAnimation(titleLabel)
createFloatAnimation(logoFrame)

-- Toggle Button (Always visible)
local toggleButton = Instance.new("TextButton")
toggleButton.Name = "ToggleButton"
toggleButton.Size = UDim2.new(0, 120, 0, 40)
toggleButton.Position = UDim2.new(1, -140, 0, 20)
toggleButton.BackgroundColor3 = Color3.fromRGB(100, 50, 255)
toggleButton.BorderSizePixel = 0
toggleButton.Text = "Hide GUI"
toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
toggleButton.TextScaled = true
toggleButton.Font = Enum.Font.GothamBold
toggleButton.Parent = screenGui

-- Toggle button styling
local toggleCorner = Instance.new("UICorner")
toggleCorner.CornerRadius = UDim.new(0, 10)
toggleCorner.Parent = toggleButton

local toggleStroke = Instance.new("UIStroke")
toggleStroke.Color = Color3.fromRGB(100, 50, 255)
toggleStroke.Thickness = 2
toggleStroke.Transparency = 0.5
toggleStroke.Parent = toggleButton

-- Toggle functionality
local function toggleGUI()
    isGUIVisible = not isGUIVisible
    
    local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut)
    
    if isGUIVisible then
        -- Show GUI
        mainFrame.Visible = true
        toggleButton.Text = "Hide GUI"
        toggleButton.BackgroundColor3 = Color3.fromRGB(100, 50, 255)
        
        local showTween = TweenService:Create(mainFrame, tweenInfo, {
            Position = UDim2.new(0, 0, 0, 0)
        })
        showTween:Play()
    else
        -- Hide GUI
        toggleButton.Text = "Show GUI"
        toggleButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
        
        local hideTween = TweenService:Create(mainFrame, tweenInfo, {
            Position = UDim2.new(0, 0, -1, 0)
        })
        hideTween:Play()
        hideTween.Completed:Connect(function()
            if not isGUIVisible then
                mainFrame.Visible = false
            end
        end)
    end
end

-- Connect toggle button
toggleButton.MouseButton1Click:Connect(toggleGUI)

-- Hover effects for toggle button
toggleButton.MouseEnter:Connect(function()
    local hoverTween = TweenService:Create(toggleButton, TweenInfo.new(0.2), {
        Size = UDim2.new(0, 130, 0, 45)
    })
    hoverTween:Play()
end)

toggleButton.MouseLeave:Connect(function()
    local leaveTween = TweenService:Create(toggleButton, TweenInfo.new(0.2), {
        Size = UDim2.new(0, 120, 0, 40)
    })
    leaveTween:Play()
end)

-- Keyboard shortcut (G key to toggle)
local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if input.KeyCode == Enum.KeyCode.G then
        toggleGUI()
    end
end)

olddiamond = game.Players.LocalPlayer:GetAttribute("Diamonds")
-- Game logic variables
local diamonds = 0
local currentDay = 1
local startTime = tick()

-- Update functions
local function updateDiamonds()
    local playerDiamonds = game.Players.LocalPlayer:GetAttribute("Diamonds") or 0
    if playerDiamonds ~= diamonds then
        diamonds = playerDiamonds
        gemsCount.Text = tostring(diamonds) .. " ( +" .. tostring(game.Players.LocalPlayer:GetAttribute("Diamonds") - olddiamond) .. " )"
        
        -- Diamond gain animation
        local originalSize = gemsCount.Size
        local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out)
        local scaleTween = TweenService:Create(gemsCount, tweenInfo, {
            Size = originalSize * 1.2
        })
        scaleTween:Play()
        scaleTween.Completed:Connect(function()
            local returnTween = TweenService:Create(gemsCount, tweenInfo, {
                Size = originalSize
            })
            returnTween:Play()
        end)
    end
end

local function updateDay()
    local storyDay = workspace:GetAttribute("StoryDayCounter") or 1
    if storyDay ~= currentDay then
        currentDay = storyDay
        dayCount.Text = tostring(currentDay)
        
        -- Day change animation
        local originalSize = dayCount.Size
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Bounce, Enum.EasingDirection.Out)
        local scaleTween = TweenService:Create(dayCount, tweenInfo, {
            Size = originalSize * 1.3
        })
        scaleTween:Play()
        scaleTween.Completed:Connect(function()
            local returnTween = TweenService:Create(dayCount, tweenInfo, {
                Size = originalSize
            })
            returnTween:Play()
        end)
    end
end

local function updateTime()
    local currentTime = tick()
    local elapsedTime = currentTime - startTime
    local minutes = math.floor(elapsedTime / 60)
    local seconds = math.floor(elapsedTime % 60)
    timeDisplay.Text = string.format("%02d:%02d", minutes, seconds)
end

-- Main update loop
RunService.Heartbeat:Connect(function()
    updateTime()
    updateDiamonds()
    updateDay()
end)

-- Utility functions for external use
function getDiamonds()
    return player:GetAttribute("Diamonds") or 0
end

function getCurrentDay()
    return workspace:GetAttribute("StoryDayCounter") or 1
end

function showGUI()
    if not isGUIVisible then
        toggleGUI()
    end
end

function hideGUI()
    if isGUIVisible then
        toggleGUI()
    end
end

function isGUIOpen()
    return isGUIVisible
end

-- Initialize displays
updateDiamonds()
updateDay()
end
